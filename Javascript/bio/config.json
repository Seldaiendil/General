{
  "name"    : "bio",

  "include" :
  [
    {
      "path" : "${QOOXDOO_PATH}/tool/data/config/application.json"
    }
  ],

  "export" :
  [
    "api",
    "api-data",
    "build",
    "clean",
    "distclean",
    "fix",
    "info",
    "inspector",
    "lint",
    "migration",
    "pretty",
    "profiling",
    "source",
    "source-all",
    "source-hybrid",
    "simulation-build",
    "simulation-run",
    "test",
    "test-source",
    "translation"
  ],
  
  "default-job" : "source",

  "let" :
  {
    "APPLICATION"  : "bio",
    "QOOXDOO_PATH" : "../extern/qooxdoo/1.5",
    "QXTHEME"      : "bio.theme.Theme",
    "API_EXCLUDE"  : ["qx.test.*", "${APPLICATION}.theme.*", "${APPLICATION}.test.*", "${APPLICATION}.simulation.*"],
    "LOCALES"      : [ "en" ],
    "CACHE"        : "${TMPDIR}/qx${QOOXDOO_VERSION}/cache",
    "ROOT"         : ".",
    "SELCHI_PATH"  : "../../selchi/src"
  },

  // You only need to edit the remainder of this file, if you want to customize
  // specific jobs, or add own job definitions.

  "jobs" :
  {
    "source-script": {
      "add-script": [
        "${SELCHI_PATH}/core/extensions.js",
        "${SELCHI_PATH}/core/core.js",
        "${SELCHI_PATH}/core/event.js",
        "${SELCHI_PATH}/classes/vector.js",
        "${SELCHI_PATH}/classes/force.js",
        "${SELCHI_PATH}/classes/phisics.js"
      ]
    },
    "build-script": {
      "add-script": [
        "${SELCHI_PATH}/core/extensions.js",
        "${SELCHI_PATH}/core/core.js",
        "${SELCHI_PATH}/core/event.js",
        "${SELCHI_PATH}/classes/vector.js",
        "${SELCHI_PATH}/classes/force.js",
        "${SELCHI_PATH}/classes/phisics.js"
      ]
    }
  }
}
